<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="miniboardSQL">
	<select id="boardList" parameterType="Map" resultType="miniboardDTO">
		select * from miniboard order by ref desc, step asc limit #{startNum}, #{endNum}
	</select>
	<select id="getTotalA" resultType="Integer">
		SELECT COUNT(*) FROM MINIBOARD
	</select>
</mapper>